{
  "manifest_version": 2,
  "name": "Cop Finder",
  "description": "Le bot Supreme le plus rapide du monde",
  "version": "1.0",
  "permissions": [
    "*://*.supremenewyork.com/*",
    "*://*.supremenewyork.com/mobile_stock.json",
    "*://gogetproxy.com/checker/*",
    "*://supremecommunity.com/*",
    "*://proxycheck.io/*",
    "*://cop-finder.com/*",
    "notifications",
    "unlimitedStorage",
    "tabs",
    "windows",
    "proxy",
    "activeTab",
    "background",
    "notifications",
    "webRequest",
    "webRequestBlocking"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/extension.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": {
    "173": "img/icon.png"
  }
}
