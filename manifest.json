{
  "manifest_version": 2,
  "name": "Cop Finder",
  "description": "Le bot Supreme le plus rapide du monde",
  "version": "1.0",
  "permissions": [
    "http://*.supremenewyork.com/*",
    "https://*.supremenewyork.com/*",
    "http://cop-finder.com/*",
    "https://cop-finder.com/*",
    "https://api.cop-finder.com/*",
    "notifications",
    "storage",
    "tabs",
    "windows",
    "activeTab",
    "background",
    "notifications",
    "webRequest"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.supremenewyork.com/*",
        "https://*.supremenewyork.com/*"
      ],
      "js": ["js/extension.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": {
    "173": "img/icon.png"
  }
}
